#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules/watch/cli.js/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules/watch/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/watch/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules:/home/aluno/Área de Trabalho/Luan-React/node_modules:/home/aluno/Área de Trabalho/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../watch/0.18.0/node_modules/watch/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules/watch/cli.js/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules/watch/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/watch/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/Área de Trabalho/Luan-React/behappy-frontend/node_modules:/home/aluno/Área de Trabalho/Luan-React/node_modules:/home/aluno/Área de Trabalho/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../watch/0.18.0/node_modules/watch/cli.js" "$@"
  ret=$?
fi
exit $ret
